{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "Node",
        "module": "CommonJS",
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./build/",
        "removeComments": true
    }
}